{
  "extends": "@tsconfig/recommended/tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "lib": [
      "es2018",
    ],
    "importHelpers": true
  },
  "exclude": [
    "node_modules",
    "dist",
  ]
}